<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="cvaltare_curriculumtag_view_tree" model="ir.ui.view">
        <field name="name">cvaltare.curriculumtag.tree</field>
        <field name="model">cvaltare.curriculumtag</field>
        <field name="arch" type="xml">
            <tree string="Curriculum Tags" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="cvaltare_curriculumtag_action" model="ir.actions.act_window">
        <field name="name">Etiquetas de curriculums</field>
        <field name="res_model">cvaltare.curriculumtag</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
